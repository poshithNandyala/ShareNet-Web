# Server Configuration
PORT=8000
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/sharenet?retryWrites=true&w=majority

# JWT Tokens
ACCESS_TOKEN_SECRET=your-secret-access-token-key-here
ACCESS_TOKEN_EXPIRY=7d
REFRESH_TOKEN_SECRET=your-secret-refresh-token-key-here
REFRESH_TOKEN_EXPIRY=30d

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret
